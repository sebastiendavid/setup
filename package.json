{
  "name": "setup",
  "version": "1.0.0",
  "description": "setup experiment",
  "main": "src/index.js",
  "scripts": {
    "build": "BABEL_DISABLE_CACHE=\"1\" NODE_ENV=\"production\" node_modules/.bin/webpack --progress -p",
    "clean": "node_modules/.bin/trash '*.log' build coverage errorShots",
    "cov": "NODE_PATH=\"$PWD\" node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- --opts .mocha",
    "lint": "node_modules/.bin/eslint *.js ./src",
    "prod": "NODE_ENV=\"production\" node_modules/.bin/webpack-dev-server --progress --colors -p",
    "start": "node_modules/.bin/webpack-dev-server --inline --hot --progress --colors -d",
    "test": "NODE_PATH=\"$PWD\" node_modules/.bin/mocha --opts .mocha",
    "test:e2e": "node_modules/.bin/wdio"
  },
  "author": "SÃ©bastien David",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "~6.3.6",
    "babel-core": "~6.9.1",
    "babel-eslint": "~6.0.4",
    "babel-loader": "~6.2.4",
    "babel-plugin-transform-remove-console": "~6.8.0",
    "babel-plugin-transform-remove-debugger": "~6.8.0",
    "babel-preset-es2015": "~6.9.0",
    "babel-preset-react": "~6.5.0",
    "babel-preset-react-optimize": "~1.0.1",
    "babel-preset-stage-0": "~6.5.0",
    "babel-register": "~6.9.0",
    "chai": "~3.5.0",
    "css-loader": "~0.23.1",
    "cssnano": "~3.7.0",
    "eslint": "~2.12.0",
    "eslint-config-airbnb": "~9.0.1",
    "eslint-plugin-import": "~1.8.1",
    "eslint-plugin-jsx-a11y": "~1.4.2",
    "eslint-plugin-react": "~5.1.1",
    "extract-text-webpack-plugin": "~1.0.1",
    "html-loader": "~0.4.3",
    "html-webpack-plugin": "~2.21.0",
    "istanbul": "~1.0.0-alpha.2",
    "mocha": "~2.5.3",
    "moment": "~2.13.0",
    "node-sass": "~3.7.0",
    "normalize.css": "~4.1.1",
    "phantomjs-prebuilt": "~2.1.7",
    "postcss-loader": "~0.9.1",
    "proxyquire": "~1.7.9",
    "react": "~15.1.0",
    "react-addons-perf": "~15.1.0",
    "react-dom": "~15.1.0",
    "react-redux": "~4.4.5",
    "react-router": "~2.4.1",
    "redux": "~3.5.2",
    "redux-thunk": "~2.1.0",
    "sass-loader": "~3.2.0",
    "selenium-standalone": "~5.1.0",
    "sinon": "~1.17.4",
    "style-loader": "~0.13.1",
    "trash-cli": "~1.3.0",
    "wdio-dot-reporter": "0.0.5",
    "wdio-mocha-framework": "~0.3.1",
    "webdriverio": "~4.0.9",
    "webpack": "~1.13.1",
    "webpack-dev-server": "~1.14.1",
    "webpack-node-externals": "~1.2.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "ignore": [
      "node_modules"
    ],
    "env": {
      "production": {
        "presets": [
          "react-optimize"
        ],
        "plugins": [
          "transform-remove-debugger",
          "transform-remove-console"
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "mocha": true
    },
    "rules": {
      "comma-dangle": [
        2,
        "never"
      ],
      "max-len": [
        2,
        120,
        2
      ],
      "no-console": 0,
      "import/no-unresolved": 0
    }
  }
