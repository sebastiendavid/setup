{
  "name": "setup",
  "version": "1.0.0",
  "description": "setup experiment",
  "main": "src/index.js",
  "scripts": {
    "build": "BABEL_DISABLE_CACHE=\"1\" NODE_ENV=\"production\" node_modules/.bin/webpack --progress -p",
    "clean": "node_modules/.bin/trash '*.log' build coverage errorShots",
    "cov": "NODE_PATH=\"$PWD\" node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- --opts .mocha",
    "lint": "node_modules/.bin/eslint *.js ./src",
    "prod": "NODE_ENV=\"production\" node_modules/.bin/webpack-dev-server --progress --colors -p",
    "start": "node_modules/.bin/webpack-dev-server --inline --hot --progress --colors -d",
    "test": "NODE_PATH=\"$PWD\" node_modules/.bin/mocha --opts .mocha",
    "test:e2e": "node_modules/.bin/wdio"
  },
  "author": "SÃ©bastien David",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "~6.3.7",
    "babel-core": "~6.11.4",
    "babel-eslint": "~6.1.2",
    "babel-loader": "~6.2.4",
    "babel-plugin-transform-class-properties": "~6.11.5",
    "babel-plugin-transform-remove-console": "~6.8.0",
    "babel-plugin-transform-remove-debugger": "~6.8.0",
    "babel-preset-es2015": "~6.9.0",
    "babel-preset-modern-browsers": "~5.0.2",
    "babel-preset-react": "~6.11.1",
    "babel-preset-react-optimize": "~1.0.1",
    "chai": "~3.5.0",
    "css-loader": "~0.23.1",
    "cssnano": "~3.7.3",
    "eslint": "~3.1.1",
    "eslint-config-airbnb": "~9.0.1",
    "eslint-import-resolver-webpack": "~0.4.0",
    "eslint-plugin-import": "~1.12.0",
    "eslint-plugin-jsx-a11y": "~2.0.1",
    "eslint-plugin-react": "~5.2.2",
    "extract-text-webpack-plugin": "~1.0.1",
    "html-loader": "~0.4.3",
    "html-webpack-plugin": "~2.22.0",
    "istanbul": "~1.1.0-alpha.1",
    "mocha": "~2.5.3",
    "moment": "~2.14.1",
    "node-sass": "~3.8.0",
    "normalize.css": "~4.2.0",
    "phantomjs-prebuilt": "~2.1.8",
    "postcss-loader": "~0.9.1",
    "proxyquire": "~1.7.10",
    "react": "~15.2.1",
    "react-addons-perf": "~15.2.1",
    "react-dom": "~15.2.1",
    "react-redux": "~4.4.5",
    "react-router": "~2.6.0",
    "redux": "~3.5.2",
    "redux-thunk": "~2.1.0",
    "sass-loader": "~4.0.0",
    "selenium-standalone": "~5.5.0",
    "sinon": "~1.17.5",
    "style-loader": "~0.13.1",
    "trash-cli": "~1.3.0",
    "wdio-dot-reporter": "0.0.6",
    "wdio-mocha-framework": "~0.3.10",
    "webdriverio": "~4.2.3",
    "webpack": "~1.13.1",
    "webpack-dev-server": "~1.14.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-class-properties"
    ],
    "ignore": [
      "node_modules"
    ],
    "env": {
      "production": {
        "presets": [
          "react-optimize"
        ],
        "plugins": [
          "transform-remove-debugger",
          "transform-remove-console"
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "plugins": [
      "import"
    ],
    "env": {
      "mocha": true
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "webpack.config.babel.js"
        }
      }
    },
    "rules": {
      "max-len": [
        2,
        120,
        2
      ],
      "no-console": 0
    }
  }
}
